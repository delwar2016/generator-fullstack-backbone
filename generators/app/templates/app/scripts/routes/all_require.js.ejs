/*global define*/

define([
    'jquery',
    'backbone',
    'views/doctor',
    'vendor/common'
], function ($, Backbone, DoctorView,Common) {
    'use strict';
    var AllRouter = Backbone.Router.extend({
        routes: {
            '':'main',
            'doctor':'doctor'
        },
        initialize:function(){
            this.back=$("#backBtn");
        },
        execute: function(callback, args, name) {
            if(Common.currentView) {
                Common.currentView.remove();
            }
            if (callback) callback.apply(this, args);
        },
        main: function () {
            this.back.hide();
            Common.currentView=new DoctorView();
        },
        doctor: function () {
            this.back.hide();
            Common.currentView=new DoctorView();
        }
    });

    return AllRouter;
});
